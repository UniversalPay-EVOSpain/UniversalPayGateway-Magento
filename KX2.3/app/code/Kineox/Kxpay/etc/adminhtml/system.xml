<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Config/etc/system_file.xsd">
    <system>
        <section id="payment">
            <group id="kxpay" translate="label" type="text" sortOrder="100" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>KXPay</label>
					<comment><![CDATA[<img src="data:image/png;base64," height="50" width="120"/>]]></comment>
           	
           		<field id="active" translate="label comment" type="select" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1">
					<label>Activar pago por KXPay</label>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
					<config_path>payment/kxpay/active</config_path>
				</field>
                <field id="environment" translate="label" type="select" sortOrder="2" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Entorno</label>
					<source_model>Kineox\Kxpay\Model\Config\Source\Environmentoptions</source_model>
					<config_path>payment/kxpay/environment</config_path>
                    <validate>required-entry</validate>
                </field>
                <field id="merchant_identifier" translate="label" type="text" sortOrder="3" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Identificador del comercio</label>
					<config_path>payment/kxpay/merchant_identifier</config_path>
                    <validate>required-entry</validate>
                </field>
                <field id="secure_key" translate="label" type="text" sortOrder="4" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Clave del comercio</label>
					<config_path>payment/kxpay/secure_key</config_path>
                    <validate>required-entry</validate>
                </field>
           		<field id="p1c" translate="label comment" type="select" sortOrder="5" showInDefault="1" showInWebsite="1" showInStore="0">
					<label>Permitir pago en 1 click</label>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
					<config_path>payment/kxpay/p1c</config_path>
				</field>
                <field id="p1c_text" translate="label" type="text" sortOrder="6" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Texto de condiciones de pago</label>
					<config_path>payment/kxpay/p1c_text</config_path>
                    <validate>required-entry</validate>
                </field>
                <field id="p1c_link" translate="label" type="text" sortOrder="7" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Enlace de condiciones de pago</label>
					<config_path>payment/kxpay/p1c_link</config_path>
                    <validate>required-entry</validate>
                </field>
                
                <field id="style_back_boton" translate="label" type="text" sortOrder="8" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Color del fondo del botón</label>
					<config_path>payment/kxpay/style_back_boton</config_path>
                    <frontend_model>Kineox\Kxpay\Block\Color</frontend_model>
                </field>
                <field id="style_color_boton" translate="label" type="text" sortOrder="9" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Color de las letras del botón</label>
					<config_path>payment/kxpay/style_color_boton</config_path>
                    <frontend_model>Kineox\Kxpay\Block\Color</frontend_model>
                </field>
                <field id="style_back_frame" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="0">
                   <label>Color del fondo del iframe</label>
					<config_path>payment/kxpay/style_back_frame</config_path>
                    <frontend_model>Kineox\Kxpay\Block\Color</frontend_model>
                </field>
                <field id="style_color_label" translate="label" type="text" sortOrder="11" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Color de las letras del iframe</label>
					<config_path>payment/kxpay/style_color_label</config_path>
                    <frontend_model>Kineox\Kxpay\Block\Color</frontend_model>
                </field>
                
           		<field id="require_cardholder" translate="label comment" type="select" sortOrder="12" showInDefault="1" showInWebsite="1" showInStore="0">
					<label>Solicitar titular de tarjeta</label>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
					<config_path>payment/kxpay/require_cardholder</config_path>
				</field>
           		<field id="active_log" translate="label comment" type="select" sortOrder="13" showInDefault="1" showInWebsite="1" showInStore="0">
					<label>Activar log</label>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
					<config_path>payment/kxpay/active_log</config_path>
				</field>
                <field id="order_status" translate="label" type="select" sortOrder="14" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Estado del pedido tras el pago</label>
					<source_model>Magento\Sales\Model\Config\Source\Order\Status</source_model>
					<config_path>payment/kxpay/order_status</config_path>
                </field>
                <field id="card" translate="label" type="select" sortOrder="15" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con tarjeta de crédito y débito</label>
					<config_path>payment/kxpay/card</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="sofort" translate="label" type="select" sortOrder="15" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con Sofort/Klarna, transferencias online</label>
					<config_path>payment/kxpay/sofort</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="correos" translate="label" type="select" sortOrder="15" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago en oficina de Correos</label>
					<config_path>payment/kxpay/correos</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="trustly" translate="label" type="select" sortOrder="16" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con Trustly, transferencias online</label>
					<config_path>payment/kxpay/trustly</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="biocryptology" translate="label" type="select" sortOrder="17" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago seguro con Biocryptology</label>
					<config_path>payment/kxpay/biocryptology</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="paypal" translate="label" type="select" sortOrder="18" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con Paypal</label>
					<config_path>payment/kxpay/paypal</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="barzahlen" translate="label" type="select" sortOrder="19" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con Barzahlen (solo Alemania)</label>
					<config_path>payment/kxpay/barzahlen</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="amazonpay" translate="label" type="select" sortOrder="20" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con Amazon Pay</label>
					<config_path>payment/kxpay/amazonpay</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="gpay" translate="label" type="select" sortOrder="21" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con Google Pay</label>
					<config_path>payment/kxpay/gpay</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="bizum" translate="label" type="select" sortOrder="22" showInDefault="1" showInWebsite="1" showInStore="0">
                   	<label>Pago con Bizum</label>
					<config_path>payment/kxpay/correos</config_path>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
